#!/bin/sh5
command=`basename $0`
case $command       in
	xfind)  /usr/local/unix/www "http://crnvmc/FIND" $@;;
	yellow) /usr/local/unix/www "http://crnvmc/FIND/yellow" $@;;
	vmnews) /usr/local/unix/www "http://crnvmc/NEWS/vmnews" $@;;
	sting)  /usr/local/unix/www "http://crnvmc/NEWS/sting" $@;;
esac
exit 0
